{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build HDC",
            "type": "shell",
            "command": "C:/msys64/mingw64/bin/gcc.exe",
            "args": [
                "-O3",
                "-I", "include",
                "src/hdc_memory.c",
                "src/hdc_encode.c",
                "src/hdc_train.c",
                "src/hdc_classify.c",
                "src/hdc_features.c",
                "src/main.c",
                "-o", "hdc_cpu.exe"
            ],
            "group": "build"
        }
    ]
}
